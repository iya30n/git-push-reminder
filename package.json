{
  "name": "git-push-reminder",
  "displayName": "git-push-reminder",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:git-push-reminder.keybindings.quit",
    "onCommand:git-push-reminder.keybindings.quitEmptyWindow",
    "onCommand:git-push-reminder.keybindings.closeWindow",
    "onCommand:git-push-reminder.keybindings.stopCloseEmptyWindow"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "git-push-reminder.keybindings.quit",
        "title": "Quit Visual Studio Code",
        "key": "ctrl+Q",
        "mac": "cmd+Q",
        "when": "git-push-reminder.isActive && isWindows && config.git-push-reminder.enableQuitKeybindingOnWindows || git-push-reminder.isActive && isMac || git-push-reminder.isActive && isLinux"
      },
      {
        "command": "git-push-reminder.keybindings.quitEmptyWindow",
        "title": "Quit Visual Studio Code",
        "key": "ctrl+Q",
        "mac": "cmd+Q",
        "when": "git-push-reminder.isActive && isWindows && config.git-push-reminder.enableQuitKeybindingOnWindows && config.git-push-reminder.closeTabShouldTryToCloseEmptyWindow && !editorIsOpen && !multipleEditorGroups || git-push-reminder.isActive && isMac && config.git-push-reminder.closeTabShouldTryToCloseEmptyWindow && !editorIsOpen && !multipleEditorGroups || git-push-reminder.isActive && isLinux && config.git-push-reminder.closeTabShouldTryToCloseEmptyWindow && !editorIsOpen && !multipleEditorGroups"
      },
      {
        "command": "git-push-reminder.keybindings.closeWindow",
        "title": "Close Window",
        "key": "ctrl+shift+W",
        "mac": "cmd+shift+W"
      },
      {
        "command": "git-push-reminder.keybindings.closeWindow",
        "title": "Close Window",
        "key": "alt+f4",
        "when": "isLinux || isWindows"
      },
      {
        "command": "git-push-reminder.keybindings.closeWindow",
        "title": "Close Editor",
        "key": "ctrl+W",
        "mac": "cmd+W",
        "when": "!editorIsOpen && !multipleEditorGroups && config.git-push-reminder.closeTabShouldTryToCloseEmptyWindow"
      },
      {
        "command": "git-push-reminder.keybindings.closeWindow",
        "title": "Close Editor",
        "key": "ctrl+f4",
        "when": "!editorIsOpen && !multipleEditorGroups && config.git-push-reminder.closeTabShouldTryToCloseEmptyWindow && isWindows"
      },
      {
        "command": "git-push-reminder.keybindings.stopCloseEmptyWindow",
        "title": "Close Editor",
        "key": "ctrl+W",
        "mac": "cmd+W",
        "when": "isMac && !editorIsOpen && !multipleEditorGroups && !config.git-push-reminder.closeTabShouldTryToCloseEmptyWindow"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "vscode-nls": "^5.2.0"
  }
}
